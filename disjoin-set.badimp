struct DSU{
	LL mxn;
	vector<LL> par, sz;

	DSU(LL ini){
		mxn = ini;
		par.assign(ini + 5, 0);
		sz.assign(ini + 5, 1);
		FOR(i, mxn) par[i] = i;
	}

	LL fnd(LL g){
		if(g == par[g]) return g;
 		return par[g] = fnd(par[g]);
	}

	void uni(LL a, LL b){
		a = fnd(a), b = fnd(b);
		if(a == b) return;

		if(sz[a] <= sz[b]) swap(a, b); // a bigger
		par[b] = a; sz[a] += sz[b];
	}
};
